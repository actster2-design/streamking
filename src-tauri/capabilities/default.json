{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Stream King desktop app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "deep-link:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://api.real-debrid.com/*" },
        { "url": "https://*.real-debrid.com/*" }
      ]
    }
  ]
}
